{
    "ver": "0.0.1",
    "authors": [
        "swwang"
    ],
    "contact": "",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameter-groups": [
        {
            "id": "amp_reduce_params",
            "parameters": [
                "abund_cutoff",
                "sd_cutoff",
                "tax_rank",
                "tax_field"
            ],
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "with_border": true
        },{
            "id": "cor_params",
            "parameters": [
                "cor_method",
                "cor_cutoff",
                "p_adj_method",
                "p_adj_cutoff"
            ],
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "with_border": true
        } 

    ],
    "parameters": [ 
        {
            "id": "amp_mat_upa",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseMatrices.AmpliconMatrix" ]
            }
        },{
            "id": "sample_metadata",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "textsubdata",
            "textsubdata_options" : {
                "show_src_obj": false,
                "allow_custom": true,
                "multiselection": false,
                "subdata_selection": {
                    "parameter_id": "amp_mat_upa",
                    "path_to_subdata": [
                        "col_attributemapping_ref",
                        "<WSREF>",
                        "attributes"
                    ],
                    "subdata_included": [
                        "attributes/[*]/attribute",
                        "attributes/[*]/source",
                        "col_attributemapping_ref"
                    ],
                    "selection_id": "attribute",
                    "selection_description": [
                        "attribute",
                        "source"
                    ],
                    "description_template": "source: {{source}}"
                }
            }
        },{
            "id": "abund_cutoff",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float": "0",
                "max_float": "1"
            }
        },{
            "id": "sd_cutoff",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float": "0"
            }
        },{
            "id": "tax_rank",
            "optional": false,
            "advanced": true,
            "allow-multiple": false,
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": "None",
                        "display": "None"
                    },{
                        "value": "species",
                        "display": "Species"
                    },{
                        "value": "genus",
                        "display": "Genus"
                    },{
                        "value": "family",
                        "display": "Family"
                    },{
                        "value": "order",
                        "display": "Order"
                    },{
                        "value": "class",
                        "display": "Class"
                    },{
                        "value": "phylum",
                        "display": "Phylum"
                    },{
                        "value": "domain",
                        "display": "Domain"
                    }
                ]
            },
            "default_values": ["None"]
        },{
            "id": "tax_field",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "field_type": "textsubdata",
            "textsubdata_options" : {
                "show_src_obj": false,
                "allow_custom": true,
                "multiselection": false,
                "subdata_selection": {
                    "parameter_id": "amp_mat_upa",
                    "path_to_subdata": [
                        "row_attributemapping_ref",
                        "<WSREF>",
                        "attributes"
                    ],
                    "subdata_included": [
                        "attributes/[*]/attribute",
                        "attributes/[*]/source",
                        "row_attributemapping_ref"
                    ],
                    "selection_id": "attribute",
                    "selection_description": [
                        "attribute",
                        "source"
                    ],
                    "description_template": "source: {{source}}"
                }
            }
        },{
            "id": "cor_method",
            "optional": false,
            "advanced": true,
            "allow-multiple": false,
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": "kendall",
                        "display": "Kendall-Tau"
                    },{
                        "value": "spearman",
                        "display": "Spearman"
                    },{
                        "value": "pearson",
                        "display": "Pearson"
                    }
                ]
            },
            "default_values": ["kendall"]
        },{
            "id": "cor_cutoff",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.5" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float": "0",
                "max_float": "1"
            }
        },{
            "id": "p_adj_method",
            "optional": false,
            "advanced": true,
            "allow-multiple": false,
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": "BH",
                        "display": "Benjamini-Hochberg"
                    },{
                        "value": "bonferroni",
                        "display": "Bonferroni"
                    }
                ]
            },
            "default_values": ["BH"]
        },{
            "id": "p_adj_cutoff",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.05" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float": "0",
                "max_float": "1"
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "OTUSampleMetadataCorrelation",
            "method": "run_OTUSampleMetadataCorrelation",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "amp_mat_upa",
                    "target_property": "amp_mat_upa",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "sample_metadata",
                    "target_property": "sample_metadata"
                },{
                    "input_parameter": "abund_cutoff",
                    "target_property": "abund_cutoff"
                },{
                    "input_parameter": "sd_cutoff",
                    "target_property": "sd_cutoff"
                },{
                    "input_parameter": "tax_rank",
                    "target_property": "tax_rank"
                },{
                    "input_parameter": "tax_field",
                    "target_property": "tax_field"
                },{
                    "input_parameter": "cor_method",
                    "target_property": "cor_method"
                },{
                    "input_parameter": "cor_cutoff",
                    "target_property": "cor_cutoff"
                },{
                    "input_parameter": "p_adj_method",
                    "target_property": "p_adj_method"
                },{
                    "input_parameter": "p_adj_cutoff",
                    "target_property": "p_adj_cutoff"
                },{
                    "input_parameter": "amp_reduce_params",
                    "target_property": "amp_reduce_params"
                },{
                    "input_parameter": "cor_params",
                    "target_property": "cor_params"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
